config_version=4

[application]

config/name="ENTROPICAL PARTY"
run/main_scene="res://game/screens/SplashScreen.tscn"
run/disable_stdout=true
config/use_custom_user_dir=true
config/custom_user_dir_name="EntropicalParty"
boot_splash/image="res://assets/tex/sky_1.png"
boot_splash/fullsize=false
config/icon="res://assets/tex/breja.png"

[autoload]

Score="*res://game/globals/Score.tscn"
Trans="*res://game/globals/Trans.tscn"
WindowManagement="*res://game/globals/WindowManagement.tscn"
Music="*res://game/globals/Music.tscn"

[display]

window/size/width=1600
window/size/height=900
mouse_cursor/custom_image="res://assets/tex/breja-cur.png"
mouse_cursor/custom_image_hotspot=Vector2( 3, 21 )
window/stretch/mode="2d"
window/stretch/aspect="keep"
window/stretch/shrink="1"

[importer_defaults]

texture={
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": true,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

